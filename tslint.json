{
  "extends": [
    "tslint:recommended",
    "rxjs-tslint-rules",
    "tslint-config-prettier"
  ],
  "rules": {
    "array-type": false,
    "curly": false,
    "interface-name": false,
    "max-classes-per-file": false,
    "member-access": false,
    "no-empty": false,
    "no-shadowed-variable": false,
    "no-var-requires": false,
    "object-literal-sort-keys": false,
    "ordered-imports": { "severity": "error", "options": {"grouped-imports": true} },
    "prefer-const": false,
    "rxjs-add": { "severity": "error" },
    "rxjs-no-unused-add": { "severity": "error" },
    "variable-name": false
  }
}
