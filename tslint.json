{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rules": {
    "array-type": false,
    "curly": false,
    "max-classes-per-file": false,
    "member-access": false,
    "no-empty": false,
    "no-shadowed-variable": false,
    "no-var-requires": false,
    "object-literal-sort-keys": false,
    "prefer-const": false,
    "variable-name": false
  }
}
